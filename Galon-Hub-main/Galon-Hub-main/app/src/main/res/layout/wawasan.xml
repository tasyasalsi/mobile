<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <ImageView
        android:contentDescription="@string/pengaturan_homepage"
        android:id="@+id/imageView"
        android:layout_width="45dp"
        android:layout_height="45dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.069"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/buttonmenu"
        />
    <ImageView
        android:contentDescription="@string/logogalonhub"
        android:id="@+id/imageView2"
        android:layout_width="69dp"
        android:layout_height="69dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.007"
        app:srcCompat="@drawable/logogalonhub"
        />
    <ImageView
        android:contentDescription="@string/foto_homepage"
        android:id="@+id/imageView9"
        android:layout_width="45dp"
        android:layout_height="46dp"
        android:layout_marginStart="110dp"
        android:layout_marginBottom="27dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.894"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/fotoprofil"
        />
    <EditText
        android:id="@+id/editTextText6"
        android:layout_width="350dp"
        android:layout_height="51dp"
        android:autofillHints="name"
        android:background="@drawable/inputlogin1"
        android:ems="10"
        android:hint="@string/cari_homepage"
        android:inputType="text"
        android:paddingStart="20dp"
        android:paddingEnd="20dp"
        android:textColorHint="#666161"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.033"
        />

    <!-- FITUR HIDRASI -->
    <!-- LAYOUT FITUR HIDRASI -->
    <View
        android:id="@+id/rectangle"
        android:layout_width="158dp"
        android:layout_height="158dp"
        android:layout_constraintBottom_toBottomOf="parent"
        android:layout_constraintEnd_toEndOf="parent"
        android:background="@drawable/rectangle3"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="167dp"
        />
    <View
        android:id="@+id/iconhidrasi"
        android:layout_width="113dp"
        android:layout_height="104dp"
        app:layout_constraintBottom_toBottomOf="@+id/texthidrasi"
        app:layout_constraintEnd_toEndOf="@+id/rectangle"
        app:layout_constraintStart_toStartOf="@+id/rectangle"
        app:layout_constraintTop_toTopOf="@+id/rectangle"
        android:background="@drawable/iconhidrasi"
        />
    <!-- TEXT FITUR HIDRASI -->
    <TextView
        android:id="@+id/texthidrasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-5dp"
        android:fontFamily="@font/outfit_semibold"
        android:text="@string/hidrasi"
        android:textColor="@color/black"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle"
        app:layout_constraintEnd_toEndOf="@+id/iconhidrasi"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="@+id/iconhidrasi"
        app:layout_constraintTop_toTopOf="@+id/rectangle"
        app:layout_constraintVertical_bias="0.941"
        />

    <!-- FITUR INSIGHT -->
    <!-- LAYOUT FITUR INSIGHT -->
    <View
        android:id="@+id/rectangle2"
        android:layout_width="158dp"
        android:layout_height="158dp"
        android:layout_constraintBottom_toBottomOf="parent"
        android:layout_constraintEnd_toEndOf="parent"
        android:background="@drawable/rectangle3"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="167dp"
        />
    <View
        android:id="@+id/iconinsight"
        android:layout_width="113dp"
        android:layout_height="104dp"
        app:layout_constraintBottom_toBottomOf="@+id/textinsight"
        app:layout_constraintEnd_toEndOf="@+id/rectangle2"
        app:layout_constraintStart_toStartOf="@+id/rectangle2"
        app:layout_constraintTop_toTopOf="@+id/rectangle2"
        android:background="@drawable/iconinsight"
        />
    <!-- TEXT FITUR INSIGHT -->
    <TextView
        android:id="@+id/textinsight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-10dp"
        android:layout_marginTop="-5dp"
        android:fontFamily="@font/outfit_semibold"
        android:text="@string/insight"
        android:textColor="@color/black"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle2"
        app:layout_constraintEnd_toEndOf="@+id/iconinsight"
        app:layout_constraintHorizontal_bias="0.546"
        app:layout_constraintStart_toStartOf="@+id/iconinsight"
        app:layout_constraintTop_toTopOf="@+id/rectangle2"
        app:layout_constraintVertical_bias="0.941"
        />

    <!-- FITUR KESEHATAN -->
    <!-- LAYOUT KESEHATAN -->
    <TextView
        android:id="@+id/kesehatan"
        android:layout_width="149dp"
        android:layout_height="20dp"
        android:fontFamily="@font/outfit_semibold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle"
        app:layout_constraintVertical_bias="0.042"
        android:text="@string/kesehatan"
        android:gravity="top"
        android:textSize="19sp"
        />
    <View
        android:id="@+id/rectangle3"
        android:layout_width="352dp"
        android:layout_height="203dp"
        android:background="@drawable/rectangle2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kesehatan"
        app:layout_constraintVertical_bias="0.089"
        />
    <View
        android:id="@+id/rectangle4"
        android:layout_width="325dp"
        android:layout_height="22dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/rectangle"
        app:layout_constraintTop_toTopOf="@+id/rectangle3"
        app:layout_constraintStart_toStartOf="@+id/rectangle3"
        app:layout_constraintEnd_toEndOf="@+id/rectangle3"
        />
    <View
        android:id="@+id/rectangle5"
        android:layout_width="325dp"
        android:layout_height="22dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="90dp"
        android:background="@drawable/rectangle"
        app:layout_constraintTop_toTopOf="@+id/rectangle3"
        app:layout_constraintStart_toStartOf="@+id/rectangle3"
        app:layout_constraintEnd_toEndOf="@+id/rectangle3"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle4"
        />
    <View
        android:id="@+id/rectangle6"
        android:layout_width="325dp"
        android:layout_height="22dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="130dp"
        android:background="@drawable/rectangle"
        app:layout_constraintTop_toTopOf="@+id/rectangle3"
        app:layout_constraintStart_toStartOf="@+id/rectangle3"
        app:layout_constraintEnd_toEndOf="@+id/rectangle3"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle5"
        />
    <View
        android:id="@+id/rectangle7"
        android:layout_width="325dp"
        android:layout_height="22dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="160dp"
        android:background="@drawable/rectangle"
        app:layout_constraintTop_toTopOf="@+id/rectangle3"
        app:layout_constraintStart_toStartOf="@+id/rectangle3"
        app:layout_constraintEnd_toEndOf="@+id/rectangle3"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle6"
        />
    <View
        android:id="@+id/rectangle8"
        android:layout_width="159dp"
        android:layout_height="23dp"
        app:layout_constraintTop_toTopOf="@+id/rectangle3"
        android:layout_marginTop="200dp"
        app:layout_constraintStart_toStartOf="@+id/rectangle3"
        app:layout_constraintEnd_toEndOf="@+id/rectangle3"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle7"
        android:background="@drawable/rectangle4"
        />
    <!-- END LAYOUT KESEHATAN -->
    <!-- TEXT FITUR KESEHATAN -->
    <TextView
        android:id="@+id/txt"
        android:layout_width="313dp"
        android:layout_height="19dp"
        android:fontFamily="@font/outfit_regular"
        android:text="@string/kesehatantxt"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle4"
        app:layout_constraintEnd_toEndOf="@+id/rectangle4"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/rectangle4"
        />
    <TextView
        android:id="@+id/txt2"
        android:layout_width="313dp"
        android:layout_height="19dp"
        android:fontFamily="@font/outfit_regular"
        android:text="@string/kesehatantxt2"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle5"
        app:layout_constraintEnd_toEndOf="@+id/rectangle5"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/rectangle5"
        />
    <TextView
        android:id="@+id/txt3"
        android:layout_width="313dp"
        android:layout_height="19dp"
        android:fontFamily="@font/outfit_regular"
        android:text="@string/kesehatantxt3"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle6"
        app:layout_constraintEnd_toEndOf="@+id/rectangle6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/rectangle6"
        />
    <TextView
        android:id="@+id/txt4"
        android:layout_width="313dp"
        android:layout_height="19dp"
        android:fontFamily="@font/outfit_regular"
        android:text="@string/kesehatantxt4"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle7"
        app:layout_constraintEnd_toEndOf="@+id/rectangle7"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/rectangle7"
        />
    <TextView
        android:id="@+id/more"
        android:layout_width="199dp"
        android:layout_height="23dp"
        android:layout_marginTop="80dp"
        android:fontFamily="@font/outfit_semibold"
        android:text="@string/more"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle7"
        app:layout_constraintEnd_toEndOf="@+id/rectangle7"
        app:layout_constraintHorizontal_bias="0.809"
        app:layout_constraintStart_toStartOf="@+id/rectangle7"
        app:layout_constraintTop_toTopOf="@+id/rectangle7"
        app:layout_constraintVertical_bias="0.53"
        />
    <!-- END TEXT KESEHATAN -->
</androidx.constraintlayout.widget.ConstraintLayout>