        android:layout_width="61dp"
        android:layout_height="61dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/borderlingkaran"
        app:layout_constraintEnd_toStartOf="@+id/textView52"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <ImageView android:contentDescription="@string/logogoogle_register"
        android:id="@+id/imageView19"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView53"
